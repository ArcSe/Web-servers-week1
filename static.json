{
  "routes": {
    "/login/*": "login.html",
    "/sample/*": "sample.html",
    "/promise/*" : "promise.html",
    "/fetch/*" : "fetch.html"
  },
  "clean_urls": true,
  
  "headers":{
    "/**":{
    "Content-Type" : "text/plain; charset=UTF-8",
    "Access-Control-Allow-Origin" : "*"
    },
    "/sample/*":{
    "Content-Type" : "text/plain; charset=UTF-8",
    "Access-Control-Allow-Origin" : "*"
    }
  }
}
